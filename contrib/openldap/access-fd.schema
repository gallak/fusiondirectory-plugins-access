# Object Class
objectclass (1.3.6.1.4.1.38414.142.4.1 NAME 'fdAccessService'
        SUP top AUXILIARY
        DESC 'FusionDirectory - AccessServer Config'
        MUST ()
        MAY (fdAccessServiceURL $ fdAccessServiceType $ fdAccessServicePassword $ fdAccessServiceUser ))

# Object Class
objectclass (1.3.6.1.4.1.38414.142.4.2 NAME 'fdAccessApplication'
        SUP top AUXILIARY
        DESC 'FusionDirectory - Access Application Config'
        MUST (fdAccessApplicationAuthType )
        MAY (fdAccessApplicationConfKey $ fdAccessEntityType $ fdAccessApplicationCASAttributes $ fdAccessApplicationSAMLAttributes $ fdAccessApplicationOIDCScopes $ fdAccessApplicationOIDCAttributes $ fdAccessApplicationAttributes $ fdAccessApplicationOIDCClientSecret $ fdAccessApplicationOIDCClientID $ fdAccessApplicationSettings $ fdAccessApplicationAuthServerDn $ fdAccessApplicationMacros $ fdAccessApplicationSAMLMetadataFile $ fdAccessApplicationScopeRules $ fdAccessApplicationService $ fdAccessEntityAuthority $ o $ mail))

